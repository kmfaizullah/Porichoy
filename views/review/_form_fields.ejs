<div class="form-group">
    <label for="title">Title</label>
    <input required value="<%=article.title%>" title ="title " type="text" name="title" id="title" class="form-control">
  </div>
  <div class="form-group">
    <label for="description">Description</label>
    <textarea name="description" id="description" class="form-control"><%=article.description %></textarea>
  </div>
  <div class="form-group">
    <label for="markdown">Full Article Link</label>
    <textarea required name="markdown" id="markdown" class="form-control"><%=article.markdown %></textarea>
  </div>
  
  <a href="/review/adminProfile" class="btn btn-secondary">Cancel</a>
  <button type="submit" class="btn btn-primary">Save</button>